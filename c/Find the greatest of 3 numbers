#include<stdio.h>

#include<conio.h>

void main()

{

int a,b,c;

printf("Enter first number");

scanf("%d",&a);

printf("Enter second Number ");

scanf("%d",&b);

printf("Enter third number");

scanf("%d",&c);

if(a>b && a>c)

{

printf("a is greatest");

}

else if (b>c && b>a)

{

printf("b is greatest");

}

else if (c>a &&c>b)

{

printf("C is greatest");

}

else

{

printf("All numbers are equal");

}

getch();

}
